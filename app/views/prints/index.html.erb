<p id="notice"><%= notice %></p>

<h1>Prints</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prints.each do |print| %>
      <tr>
        <td><%= print.title %></td>
        <td><%= link_to 'Add Prints', edit_project_path(print) %></td>
        <td><%= link_to 'Destroy', print, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Print', new_print_path %>
